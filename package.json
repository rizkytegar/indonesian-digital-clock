  "version": "1.0.0",
  "description": "Simple Digital Clock with ReactJS",
  "author": "rizkytegar",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tailwind": "npx tailwindcss -i ./src/App.css -o ./src/style.css --watch ",
